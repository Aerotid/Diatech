                    .main-first-screen-wrapper.section
                        +main-first-screen({'data-full-screen': '', class: 'main-first-screen_padding-full'})
                            +set('bg')
                                .main-first-screen__bg-wrapper
                            +set('left-top')
                                +info-block-1('<div data-text-part>ДИАТЕХ.</div><div data-text-part>Там, где другие</div><div data-text-part>бессильны</div>', 'Спустя 22&nbsp;года на&nbsp;рынке технической диагностики и&nbsp;экспертизы промышленной безопасности мы&nbsp;остаемся новаторами в&nbsp;сфере неразрушающего контроля', null, {}, {class: 'main-first-screen__info-block-1'})
                            +set('additionals')
                                +scroll-down({class: 'main-first-screen__scroll-down'})
                    +main-slider({'data-fs-part': '', class: 'section'})
                        +main-slider-slide({
                            'data-full-screen': '',
                            'data-id': 0,
                            class: 'main-slider-slide_padding-full main-slider-slide_active main-slider-slide__slide-1'
                        })
                            +set('bg')
                                .main-slider-slide__bg-wrapper
                                    img.main-slider-slide__bg(data-img="scene-01" src='data:image/gif;base64,R0lGODlhAQABAIAAAAAAAP///yH5BAEAAAAALAAAAAABAAEAAAIBRAA7',
